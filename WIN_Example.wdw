     name : HoverChart
     internal_properties : CAAAAAgAAADA4iRlJPfTT93c1EbZPFe7NiwpiiiLv8uDoJ79kZ2I1Doto0b4F7dxmR1f9rcOHMbTvKLZPd7RxUTAs9RjUOiiEdE38V6TeJdfLh9Tv1IuwDBgz/F6ycsbBqcIaZpFJqBvc4sf5d7Xv2xD5GxpZ8uLdRYbqBdgbsRq04hZemxVEbwzzBKUO63X1jdHUq9xZOv4YZBN5Q==
     procedure_id : 1224960913778245746
     type_code : 14
     code : |1-
      // Summary: Callback for hovering the chart
      // Syntax:
      //HoverChart (<sListCategories> is string)
      //
      // Parameters:
      //	sListCategories (string): List of hovered categories (in the following format: <Series> ; <Categories> TAB ...)
      // Return value:
      // 	None
      //
      //	sListSeries (string): List of hovered series (separated by <TAB>)
      Procedure HoverChart(LOCAL sListCategories is string)
      
      	sListInfo is string
      	sCategory	is string
      	nCategory	is int
      	nSeries		is int
      	rValue		is real
      	dDate		is date
      	
      	FOR EACH string sCategory of sListCategories
      		
      		// Extracts the series and the category
      		nSeries 		= ExtractString(sCategory, 1, “;”)
      		nCategory 	= ExtractString(sCategory, 2, “;”)
      		
      		// Retrieves the value
      		rValue = CHART_STOCKSHARE[nSeries][nCategory]
      		
      		HReadFirst(Quotes, date)
      		HForward(Quotes, date, (nCategory - 1))
      		
      		// Retrieves the date of the value
      		dDate = Quotes.Date
      		
      		sListInfo += [CR] + DateToString(dDate) + “: (” + CHART_STOCKSHARE[nSeries]..Legend + “) ” + gFontBold(True) + NumToString(rValue, “.2f”) + “€” + gFontBold(False)
      	END
      	
      	IF sListInfo <> “” THEN
      		sListInfo = “Data:” + CR + sListInfo
      	END
      	
      	IF STC_INFO <> sListInfo THEN
      		STC_INFO = sListInfo
      	END
      	
