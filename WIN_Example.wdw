         	ExecuteProcess(RADIO_TYPE, trtChange)
        type : 14
      -
        internal_properties : CAAAAAgAAACjlbrhfUsXBs/c72oETyApwx1c7MiN8/tf0lNguKUPHyHMDjiDIDG9+DOTJCgGjV5XegoAIHQa3Cbl0AW4OMacFekcQ10UsPBMVopkrnpAUvNBIDtSkz5faDB1DG8Jv2y9rY8Zumz2CszuH0Q=
        code : |1-
         
         	// Changes the image
         	IMG_CHART = “chart-” + RADIO_TYPE + “.gif”
         	
         	SWITCH RADIO_TYPE
         		
         		// Line type
         		CASE 1
         			
         			// Modifies the type
         			grType(CHART_STOCKSHARE, grCurve)
         
         			// Restores the color of the axes
         			grColor(CHART_STOCKSHARE, grColorAxis, DefaultColor)
         			GR_OPACITY..Visible = False
         			
         			IF gpclDrawing <> Null THEN gpclDrawing:ModifyFlag(1, 62, “B”, True)
         			
         		// Area
         		CASE 2
         			
         			// Modifies the type
         			grType(CHART_STOCKSHARE, grArea)
         			
         			// Restores the color of the axes
         			grColor(CHART_STOCKSHARE, grColorAxis, DefaultColor)
         			GR_OPACITY..Visible = True
         			
         			IF gpclDrawing <> Null THEN gpclDrawing:ModifyFlag(1, 62, “B”, True)
         			
         		// Radar
         		CASE 3
         			
         			// Modifies the type
         			grType(CHART_STOCKSHARE, grRadar)
         			
         			// Changes the color of the axes so that they blend with the chart background
         			grColor(CHART_STOCKSHARE, grColorAxis, CHART_STOCKSHARE..BrushColor)
         			GR_OPACITY..Visible = True
         			
         			IF gpclDrawing <> Null THEN gpclDrawing:ModifyFlag(1, 62, “B”, True)			
         			
         		// Histogram
         		CASE 4
         			
         			// Modifies the type
         			grType(CHART_STOCKSHARE, grColumn)
         
         			// Restores the color of the axes
         			grColor(CHART_STOCKSHARE, grColorAxis, DefaultColor)			
         			GR_OPACITY..Visible = True
         			
         			IF gpclDrawing <> Null THEN gpclDrawing:ModifyFlag(1, 62, “B”, False)
         			
         	END
         	
         	// Drawing
         	grDraw(CHART_STOCKSHARE)
        type : 17
