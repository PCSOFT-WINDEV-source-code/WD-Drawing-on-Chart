     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //OnFlag (<sSymbol> is string)
      //
      // Parameters:
      //	sSymbol (string): Symbol associated with the flag
      // Return value:
      // 	None
      //
      Procedure OnFlag(LOCAL sSymbol is string)
      
      	nSubscript is int
      	nSubscript = LooperSeek(ATT_SYMBOL, sSymbol)
      	
      	IF nSubscript > 0 THEN		
      		LooperSelectPlus(LOOP_FLAGS, nSubscript)
      	END
     type : 458752
