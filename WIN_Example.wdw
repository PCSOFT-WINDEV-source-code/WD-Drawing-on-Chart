   -
     code : |1-
      
      gpclDrawing is cChartDrawing dynamic
      
      // Display the values over a single year
      HFilter(Quotes, date, “20080301”, “20090331”)
      
      // Counts the number of categories once the filter is implemented
      gnNbCat is int
      FOR EACH Quotes 
      	gnNbCat++
      END
      
      // Modify the opacity of the chart control in order for the grid lines to appear behind the areas.
      // Indeed, ..Opacity does not affect the axes and the grid lines of a chart
      CHART_STOCKSHARE..Opacity = 25
      
      // Defines the origin
      grOrigin(CHART_STOCKSHARE, 25, 75, grYCoordinate)
      grParameter(CHART_STOCKSHARE, grOptimizedMarksX, True)
      grParameter(CHART_STOCKSHARE, grOptimizedMarksY, True)
      
      // Modifies the color of the border for the legend
      grColor(CHART_STOCKSHARE, grColorLegend, LightGray)
   -
     code : |1-
      
      // Enables the drawing on the chart
      gpclDrawing <- grEnableDrawing(CHART_STOCKSHARE,gnNbCat)
      
      // Checks whether the drawing was enabled
      IF gpclDrawing = Null THEN
      	Error(“Unable to enable the drawing”)
      	Close()
      END
      
      gpclDrawing:CallBackRollover 	= “WIN_Example.HoverChart”
      gpclDrawing:FlagCallback 	= “WIN_Example.OnFlag”
      gpclDrawing:AreaCallback 	= “WIN_Example.OnArea”
      
      // Adds events
      AddEvents()
      
      // Update the options
      ExecuteProcess(CBOX_TRACKER, trtChange)
